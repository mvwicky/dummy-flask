{
  "name": "holdmypics",
  "version": "0.15.2",
  "description": "Placeholder image generator",
  "repository": "https://github.com/mvwicky/holdmypics.git",
  "license": "MIT",
  "author": "Michael Van Wickle <mvanwickle@gmail.com>",
  "main": "./src/main.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.ts",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.ts --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.ts --progress",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.ts --progress",
    "prod": "cross-env NODE_ENV=production webpack --config webpack.config.ts --progress",
    "test": "ts-node bin/run-tests.ts",
    "watch": "yarn run build:dev --watch"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 2,
    "overrides": [
      {
        "files": "*.ejs",
        "options": {
          "requirePragma": true
        }
      },
      {
        "files": [
          "**/*.jinja"
        ],
        "options": {
          "requirePragma": true,
          "printWidth": 120
        }
      }
    ]
  },
  "dependencies": {
    "friendly-words": "^1.1.10",
    "rfs": "^9.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@cypress/webpack-preprocessor": "^5.1.1",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@popperjs/core": "^2.0.6",
    "@types/autoprefixer": "^9.7.1",
    "@types/clipboard": "^2.0.1",
    "@types/debug": "^4.1.5",
    "@types/feather-icons": "^4.7.0",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^13.1.8",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.2",
    "@types/workbox-webpack-plugin": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "bourbon": "^7.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "clipboard": "^2.0.4",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "cypress": "^4.1.0",
    "debug": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "feather-icons": "^4.24.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pattern.css": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.0",
    "sanitize.css": "^11.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "sass-mq": "^5.0.1",
    "stylelint": "^13.3.3",
