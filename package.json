    "tippy.js": "^6.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.6.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "engines": {
    "node": "12.16.x",
    "npm": "6.14.x",
    "yarn": "1.x"
  },
  "entry": {
    "main": "./src/ts/main.ts"
  }
}
